{
  "name": "qing-dialog",
  "version": "0.0.0",
  "description": "Dialog component for qing",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/qing-dialog",
  "bugs": "https://github.com/mgenware/qing-dialog/issues",
  "keywords": [
    "dialog",
    "modal view"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/qing-dialog.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "karma start",
    "build": "cross-env NODE_ENV=production yarn test",
    "compile": "tsc -b src",
    "dev": "cross-env NODE_ENV=development yarn run clean && tsc -b src -w",
    "prepublishOnly": "yarn run test",
    "lint": "eslint --ext .ts src/ && lit-analyzer \"src/**/*.ts\" --strict",
    "clean": "del-cli dist",
    "demo": "owc-dev-server --app-index ./demo/demo.html --open --watch"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.4.2",
    "@open-wc/testing-karma": "^3.2.17",
    "@types/node": "^12.12.16",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-plugin-bare-import-rewrite": "^1.5.1",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "eslint": "^6.7.2",
    "karma": "^4.4.1",
    "lit-analyzer": "^1.1.9",
    "lit-element": "^2.2.1",
    "owc-dev-server": "^1.0.1",
    "typescript": "^3.7.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "lit-button": "^3.1.2"
  }
}
