{
  "name": "qing-dialog-component",
  "version": "0.2.0",
  "description": "Dialog component for qing",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/qing-dialog-component",
  "bugs": "https://github.com/mgenware/qing-dialog-component/issues",
  "keywords": [
    "dialog",
    "modal view"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/qing-dialog-component.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "karma start",
    "build": "cross-env NODE_ENV=production yarn test",
    "compile": "tsc -b tests",
    "dev": "cross-env NODE_ENV=development yarn run clean && tsc -b tests -w",
    "prepublishOnly": "yarn run build",
    "lint": "eslint --ext .ts src/ tests/ && lit-analyzer \"src/**/*.ts\" --strict",
    "clean": "del-cli dist",
    "serve": "owc-dev-server --app-index ./examples/examples.html --open --watch"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/dist/**/*.js",
    "/dist/**/*.ts",
    "/dist/**/*.map"
  ],
  "devDependencies": {
    "@open-wc/testing": "^2.5.4",
    "@open-wc/testing-karma": "^3.2.42",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-plugin-bare-import-rewrite": "^2.0.0",
    "cross-env": "^7.0.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "karma": "^4.4.1",
    "lit-analyzer": "^1.1.9",
    "lit-button": "^4.1.0",
    "lit-element": "^2.3.1",
    "owc-dev-server": "^1.0.1",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "lit-button": "^4"
  }
}
